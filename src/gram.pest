// Column Guide:
// Rules   =  values
WHITESPACE = _{ " " | "\t" }
char       =  { ASCII_ALPHANUMERIC | "." | "," | "!" | "?" | "'" | "\"" | "-" | ":" | ";" }
label      = @{ (char | WHITESPACE)* }
goal       =  { "/" ~ label ~ "/" }
risk       =  { "\\" ~ label ~ "\\" }
agent      =  { "<" ~ label ~ ">" }
entry      =  { goal | risk | agent }
file       =  { SOI ~ (entry | NEWLINE)* ~ EOI }
